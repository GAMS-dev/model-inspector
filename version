# Current GAMS Model Inspector version.
MI_MAJOR_VERSION=0
MI_MINOR_VERSION=5
MI_PATCH_LEVEL= 5
VERSION='$$MI_MAJOR_VERSION'.'$$MI_MINOR_VERSION'.'$$MI_PATCH_LEVEL'

# Minimum required GAMS Distribution version, which
# is the latest official GAMS release.
GAMS_DISTRIB_MAJOR=48
GAMS_DISTRIB_MINOR=1
GAMS_DISTRIB_GOLD=0
GAMS_DISTRIB_VERSION=$$GAMS_DISTRIB_MAJOR$$GAMS_DISTRIB_MINOR$$GAMS_DISTRIB_GOLD

# Project file version defines
DEFINES += 'MI_VERSION=\\"$$VERSION\\"'
DEFINES += 'MI_MAJOR_VERSION=$$MI_MAJOR_VERSION'
DEFINES += 'MI_MINOR_VERSION=$$MI_MINOR_VERSION'
DEFINES += 'MI_PATCH_LEVEL=$$MI_PATCH_LEVEL'
DEFINES += 'GAMS_DISTRIB_MAJOR=$$GAMS_DISTRIB_MAJOR'
DEFINES += 'GAMS_DISTRIB_MINOR=$$GAMS_DISTRIB_MINOR'
DEFINES += 'GAMS_DISTRIB_VERSION=$$GAMS_DISTRIB_VERSION'
DEFINES += 'GAMS_DISTRIB_VERSION_SHORT=\\"'$$GAMS_DISTRIB_MAJOR'.'$$GAMS_DISTRIB_MINOR'\\"'
